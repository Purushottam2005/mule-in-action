<?xml version="1.0" encoding="UTF-8"?>
<mule xmlns="http://www.mulesource.org/schema/mule/core/2.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:http="http://www.mulesource.org/schema/mule/http/2.0"
	xmlns:jms="http://www.mulesource.org/schema/mule/jms/2.0"
	xmlns:spring="http://www.springframework.org/schema/beans"
	xmlns:client="http://www.mulesource.org/schema/mule/client/2.0"
	xsi:schemaLocation="
       http://www.mulesource.org/schema/mule/core/2.0 http://www.mulesource.org/schema/mule/core/2.0/mule.xsd
       http://www.mulesource.org/schema/mule/http/2.0 http://www.mulesource.org/schema/mule/http/2.0/mule-http.xsd
       http://www.mulesource.org/schema/mule/jms/2.0 http://www.mulesource.org/schema/mule/jms/2.0/mule-jms.xsd
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
       http://www.mulesource.org/schema/mule/client/2.0 http://www.mulesource.org/schema/mule/client/2.0/mule-client.xsd">

	<!-- TODO secure -->
	<client:remote-dispatcher-agent>
		<client:remote-endpoint address="http://localhost:8181"
			synchronous="true" />
		<client:xml-wire-format />
	</client:remote-dispatcher-agent>

	<spring:bean name="amqConnectionFactory"
		class="org.apache.activemq.spring.ActiveMQConnectionFactory">
		<spring:property name="brokerURL" value="vm://localhost" />
	</spring:bean>

	<jms:activemq-connector name="jmsConnector"
		specification="1.1" connectionFactory-ref="amqConnectionFactory" />

</mule>
